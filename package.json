{
  "name": "website-source",
  "version": "1.0.0",
  "description": "Source code for my GitHub Pages website",
  "private": true,
  "scripts": {
    "test": "npm run clean && npm run lint",
    "build": "npm run clean && eleventy && webpack --mode=production && rm -rf ./_site/junk",
    "start:dev": "npm run build && webpack --mode=development & ELEVENTY_ENV=development eleventy --serve ",
    "start": "npm run build && webpack --mode=production && ELEVENTY_ENV=production eleventy --serve ",
    "clean": "rm -rf ./_site && npm install",
    "lint": "eslint --fix . && prettier -w .",
    "publish": "npm run build && rsync -aq --exclude='*JUNK.js' --exclude='*.DS_Store' ./_site/ ../website-output"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CalebBurton/website-source.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CalebBurton/website-source/issues"
  },
  "homepage": "https://github.com/CalebBurton/website-source#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "clean-css": "^4.2.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "html-minifier": "^4.0.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "sass-loader": "^8.0.2",
    "terser": "^4.6.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "handlebars": "^4.5.3"
  }
}
