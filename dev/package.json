{
  "name": "website-source",
  "version": "1.0.0",
  "description": "Source code for my GitHub Pages website",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run lint && webpack --mode=production",
    "build:dev": "npm run clean && npm run lint && NODE_HEAPDUMP_OPTIONS=nosignal webpack --mode=development",
    "clean": "rm -rf ./dist",
    "lint": "eslint --fix ./src/.",
    "publish": "npm run build && rm -rf ../prod/ && rsync -aq --exclude='*JUNK.js' --exclude='*.DS_Store' ./dist/ ../prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CalebBurton/website-source.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CalebBurton/website-source/issues"
  },
  "homepage": "https://github.com/CalebBurton/website-source#readme",
  "devDependencies": {
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.3.0",
    "handlebars-webpack-plugin": "^1.6.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "handlebars": "^4.2.0"
  }
}
